 Guid="c3ee7985-d1ba-4ee5-bd00-3185dd3fe4e8"
>
<Comment></Comment>
<PreConditions>
</PreConditions>
<Actions>
<Action
 Guid="b501096d-5057-466d-a460-16d5c836ceed"
><Comment></Comment>
APPLY Kernel.TrackDescription.TrackConditions.General.TCDisplay.DisplayAnnouncement(X)
ON Kernel.TrackDescription.TrackConditions.General.TCDisplay.Announcements | 
(
  Kernel.TrackDescription.TrackConditions.General.TCDisplay.IsInSoundHornGroup(X.Type)
    AND  
  Kernel.TrackDescription.TrackConditions.General.TCDisplay.ActiveConditionStart(X)
