<Action
 Guid="3c9a6e23-bdb4-41fb-9f85-d007c79c450e"
>EURORADIO.CurrentRBCConnection().SendMARequest
(
    aReason =&gt; Messages.Q_MARQSTREASON.Time_before_a_section_timer_LOA_speed_timer_expires_reached,
