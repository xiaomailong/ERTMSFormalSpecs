<PreConditions>
<PreCondition
 Guid="32291f91-c8f4-4551-9efb-1784c4b3c42b"
>Available(LevelTransitionOrder)
  AND  
LevelTransitionOrder.Location &lt;= TrainPosition.FrontEndPosition(DistanceInterval.Nom)</PreCondition><PreCondition
 Guid="a793711a-f9af-4251-b86a-906d92f0b23f"
>Available(LevelTransitionToProcess)
  AND  
Kernel.Level.Value != LevelTransitionToProcess.TRLevel</PreCondition><PreCondition
 Guid="25250332-c8c8-4785-908c-5901c81268aa"
>Mode not in [Mode.SH, Mode.PS]</PreCondition></PreConditions>
